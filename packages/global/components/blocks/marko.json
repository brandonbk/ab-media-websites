{
  "taglib-imports": [
    "./promos/marko.json"
  ],
  "<global-content-body-with-tracking>": {
    "template": "./content-body-with-tracking.marko",
    "@content": {
      "type": "object",
      "required": true
    },
    "@block-name": {
      "type": "string",
      "required": true
    }
  },
  "<global-leaderboard-ad-block>": {
    "template": "./leaderboard-ad.marko"
  },
  "<global-directory-featured-projects-block>": {
    "template": "./directory-featured-projects.marko",
    "@graphql-client": "object",
    "@short-ids": "string"
  },
  "<global-leaders-featured-projects-block>": {
    "template": "./leaders-featured-projects.marko",
    "@graphql-client": "object",
    "@short-ids": "string"
  },
  "<global-sections-listed-in-block>": {
    "template": "./sections-listed-in.marko",
    "@schedules": "array",
    "@root-alias": "string",
    "@full-name-find": "string",
    "@full-name-replace": "string",
    "@title": "string"
  },
  "<global-premium-partners-block>": {
    "template": "./premium-partners.marko"
  },
  "<global-reader-rigs-submit-block>": {
    "template": "./reader-rigs-submit.marko"
  },
  "<global-company-search-block>": {
    "template": "./company-search.marko"
  },
  "<global-section-search-block>": {
    "template": "./section-search.marko"
  }
}
